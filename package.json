  "dependencies": {
    "bootstrap": "5.1.3",
    "bootstrap-icons": "1.8.1",
    "debug": "4.3.3",
    "express": "4.17.3",
    "jquery": "3.6.0",
    "morgan": "1.10.0",
    "json-server": "0.17.0"
  },
  "devDependencies": {
    "cpx": "1.5.0",
    "eslint": "8.11.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.25.4",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "sass": "1.49.9"
  }
